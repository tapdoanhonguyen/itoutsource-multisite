<div class="bizweb-product-reviews-module"></div>
      <footer class="footer" data-lazyload2="{NV_BASE_SITEURL}themes/{TEMPLATE}/images/bg-footer.jpg" style='background-image: url("{NV_BASE_SITEURL}themes/{TEMPLATE}/images/loading.svg");'>
         <div class="back-to-top"><i class="fa fa-angle-double-up"></i></div>
		 
         <div class="site-footer">
		 <div class="wraper">
            <div class="container">
               <div class="footer-inner">
                  <div class="row">
                       
                     <div class="block block-cs col-xs-24 col-sm-10 col-md-12 col-lg-12">
                        <div class="footer-widget">
                           [MENU_FOOTER]
                        </div>
                     </div>
                     <div class="block block-cs col-xs-24 col-sm-6 col-md-6 col-lg-6">
                        <div class="footer-widget">
                           [FEATURED_PRODUCT]
                        </div>
                     </div>
                     <div class="block block-cs col-xs-24 col-sm-8 col-md-6 col-lg-6">
                        <div class="footer-widget">
                             <!-- BEGIN: imageft -->
                        <a title="{SITE_NAME}" href="{THEME_SITE_HREF}" class="logo-wrapper">		
                        <img class="img-responsive" src="{LOGO_SRC}"  alt="{SITE_NAME}" />
                        </a>
						<!-- END: imageft -->
                          [COMPANY_INFO]
                        </div>
                     </div>
                  </div>
               </div>
            </div>
         </div>
         <div class="copyright clearfix">
            <div class="container">
               <div class="inner clearfix">
                  <div class="row">
                     <div class="col-xs-24">
                        <div class="text-center copy-right-text">
                           [FOOTER_SITE]
                        </div>
                     </div>
                  </div>
               </div>
            </div>
         </div>
		 </div>
      </footer>
	  {ADMINTOOLBAR}
      <div id="myModal" class="modal fade" role="dialog"></div>
	  <!-- SiteModal Required!!! -->
      <div id="sitemodal" class="modal fade" role="dialog">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-body">
                    <em class="fa fa-spinner fa-spin">&nbsp;</em>
                </div>
                <button type="button" class="close" data-dismiss="modal"><span class="fa fa-times"></span></button>
            </div>
        </div>
    </div>