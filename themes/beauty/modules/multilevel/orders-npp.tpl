<!-- BEGIN: main --><link type="text/css" href="{NV_BASE_SITEURL}{NV_ASSETS_DIR}/js/ui/jquery.ui.core.css" rel="stylesheet" /><link rel="stylesheet" type="text/css" href="{NV_BASE_SITEURL}{NV_ASSETS_DIR}/js/jquery-ui/jquery-ui.min.css"><script type="text/javascript" src="{NV_BASE_SITEURL}{NV_ASSETS_DIR}/js/jquery-ui/jquery-ui.min.js"></script><script type="text/javascript" src="{NV_BASE_SITEURL}{NV_ASSETS_DIR}/js/language/jquery.ui.datepicker-{NV_LANG_INTERFACE}.js"></script><div class="page-wrapper">	<div class="page-content-wrapper">		<div class="page-content">			<div class="page-breadcrumb d-none d-md-flex align-items-center mb-3">				<div class="breadcrumb-title pr-3">Quản trị hệ thống</div>				<div class="pl-3">					<nav aria-label="breadcrumb">						<ol class="breadcrumb mb-0 p-0">							<li class="breadcrumb-item"><a href="javascript:;"><i class='bx bx-home-alt'></i></a>							</li>							<li class="breadcrumb-item active" aria-current="page">Đơn hàng CĐ đang xuất</li>						</ol>					</nav>				</div>			</div>			<div class="card">				<div class="card-body">					<h4 class="mb-0">Đơn hàng CĐ đang xuất</h4>					<hr/>					<form action="{NV_BASE_ADMINURL}index.php" method="GET">						<input type="hidden" name ="{NV_NAME_VARIABLE}"value="{MODULE_NAME}" />						<input type="hidden" name ="{NV_OP_VARIABLE}"value="{OP}" />						<div class="form-row">							<div class="col-xs-12 col-md-4">								<div class="form-group">									<label>{LANG.order_code}</label>									<input type="text" name="order_code" value="{SEARCH.order_code}" class="form-control" placeholder="{LANG.order_code}">								</div>							</div>							<div class="col-xs-12 col-md-4">								<label>{LANG.date_from}</label>								<div class="input-group">									<input type="text" name="from" id="from" value="{SEARCH.date_from}" class="form-control" placeholder="{LANG.date_from}" readonly="readonly">									<div class="input-group-append">										<button class="btn btn-primary" type="button"><i class="lni lni-calendar"></i></button>									</div>								</div>							</div>							<div class="col-xs-12 col-md-4">								<label>Đến ngày</label>								<div class="input-group">									<input type="text" name="to" id="to" value="{SEARCH.date_to}" class="form-control" placeholder="Đến ngày" readonly="readonly">									<div class="input-group-append">										<button class="btn btn-primary" type="button"><i class="lni lni-calendar"></i></button>									</div>								</div>							</div>							<div class="col-xs-12 col-md-4">								<div class="form-group">									<label>{LANG.order_email}</label>									<input type="email" name="order_email" value="{SEARCH.order_email}" class="form-control" placeholder="{LANG.order_email}">								</div>							</div>							<div class="col-xs-12 col-md-4">								<div class="form-group">									<label>{LANG.order_payment}</label>									<select class="form-control" name="order_payment">										<option value="">{LANG.order_payment}</option>										<!-- BEGIN: transaction_status -->										<option value="{TRAN_STATUS.key}" {TRAN_STATUS.selected}>{TRAN_STATUS.title}</option>										<!-- END: transaction_status -->									</select>								</div>							</div>							<div class="col-xs-12 col-md-4">								<label>&nbsp;</label>								<div class="input-group">									<input type="hidden" name ="checkss" value="{CHECKSESS}" />									<button type="submit" class="btn btn-primary" name="search">										<i class="lni lni-search-alt mr-1"></i>{LANG.search}									</button>								</div>							</div>						</div>					</form>					<p class="pull-right">						<strong>{LANG.siteinfo_order}: </strong>{ORDER_INFO.num_items}</span> - 						<strong>{LANG.order_total}: </strong>{ORDER_INFO.sum_price} {ORDER_INFO.sum_unit}</span>					</p>					<div class="clearfix"></div>					<!-- BEGIN: data -->					<div class="table-responsive">						<table class="table table-bordered table-hover">							<thead class="thead-dark">								<tr>									<th width="10px" class="text-center">ID</th>									<th>{LANG.order_code}</th>									<th>{LANG.order_name}</th>									<th>{LANG.order_time}</th>									<th>{LANG.order_email}</th>									<th align="right">{LANG.order_total}</th>									<th align="right">{LANG.price_payment}</th>									<th>{LANG.feeship}</th>									<th>{LANG.order_payment}</th>									<th width="130px" class="text-center">{LANG.function}</th>								</tr>							</thead>							<tbody>								<!-- BEGIN: row -->								<tr id="{DATA.order_id}">									<td><input type="checkbox" class="ck" value="{DATA.order_id}" /></td>									<td><a href="{link_view}" title="">{DATA.order_code}{DATA.ordertype}</a> {DATA.shipcode}</td>										<td>{DATA.order_name}</td>									<td>{DATA.order_time}</td>									<td><a href="{DATA.link_user}" style="text-decoration:underline" target="_blank">{DATA.order_email}</a></td>									<td align="right">{DATA.order_total} {DATA.unit_total}</td>									<td align="right">{DATA.price_payment} {DATA.unit_total}</td>									<td align="right">{DATA.feeship} {DATA.unit_total}</td>									<td>{DATA.status_payment}</td>									<td class="text-center">										<a class="btn btn-primary btn-sm " data-toggle="tooltip" data-placement="top" data-original-title="{LANG.view}" href="{link_view}" title="{LANG.view}"><i class="lni lni-eye"></i></a>									</td>								</tr>								<!-- END: row -->							</tbody>							<tfoot>								<tr>									<td colspan="2">										<i class="fa fa-check-square-o">&nbsp;</i><a href="#" id="checkall">{LANG.prounit_select}</a> -										<i class="fa fa-square-o">&nbsp;</i><a href="#" id="uncheckall">{LANG.prounit_unselect}</a>									</td>									<td colspan="8">										<a class="btn btn-success" href="javascript:voi(0);" id="export_select"><i class="lni lni-exit-down mr-1"></i>{LANG.export_select}</a>										<a class="btn btn-info" href="javascript:voi(0);" id="export_all"><i class="lni lni-exit-down mr-1"></i>{LANG.export_all}</a>									</td>								</tr>							</tfoot>						</table>					</div>					<div class="text-center">						{PAGES}					</div>				</div>			</div>		</div>	</div></div><script type='text/javascript'>    $(function() {        $('#checkall').click(function() {            $('input:checkbox').each(function() {                $(this).attr('checked', 'checked');            });            return false;        });        $('#uncheckall').click(function() {            $('input:checkbox').each(function() {                $(this).removeAttr('checked');            });            return false;        });        $('#export_select').click(function() {            var listall = [];            $('input.ck:checked').each(function() {                listall.push($(this).val());            });            if (listall.length < 1) {                alert("{LANG.prounit_del_no_items}");                return false;            }            if (confirm('{LANG.nv_is_export_confirm}')) {                $.ajax({                    type : 'POST',                    url : '{URL_EXPORT}',                    data : 'listall=' + listall,                    dataType: "json",                    success : function(response) {                        if (response.status == "OK") {                            $("#loading_bar").hide();                            alert(response.mess);                            window.location.href = script_name + '?' + nv_name_variable + '=' + nv_module_name + '&' + nv_fc_variable + '=export&step=2';                        } else {                            $("#loading_bar").hide();                            alert(response.mess);                            window.location.href = script_name + '?' + nv_name_variable + '=' + nv_module_name + '&' + nv_fc_variable + '={OP}';                        }                    }                });            }            return false;        });        $('#export_all').click(function() {            if (confirm('{LANG.nv_is_export_confirm}')) {                $.ajax({                    type : 'POST',                    url : '{URL_EXPORT}',                    data : '',                    dataType: "json",                    success : function(response) {                        if (response.status == "OK") {                            $("#loading_bar").hide();                            alert(response.mess);                            window.location.href = script_name + '?' + nv_name_variable + '=' + nv_module_name + '&' + nv_fc_variable + '=export&step=2';                        } else {                            $("#loading_bar").hide();                            alert(response.mess);                            window.location.href = script_name + '?' + nv_name_variable + '=' + nv_module_name + '&' + nv_fc_variable + '={OP}';                        }                    }                });            }            return false;        });        $('a.delete').click(function() {            if (confirm("{LANG.prounit_del_confirm}")) {                var href = $(this).attr('href');                var id = $(this).parents().parents().attr('id');                $.ajax({                    type : 'POST',                    url : href,                    data : '',                    success : function(data) {                        $('#'+id).remove();                    }                });            }            return false;        });        $("#from, #to").datepicker({            dateFormat : "dd/mm/yy",            changeMonth : true,            changeYear : true,            showOtherMonths : true,            showOn : 'focus'        });        $('#to-btn').click(function(){            $("#to").datepicker('show');        });        $('#from-btn').click(function(){            $("#from").datepicker('show');        });    });</script><!-- END: data --><!-- END: main -->